=\"slider\" :style=\"'transform: rotateY(' + curveSlider + 'deg)'\">\n                <template v-for=\"stack in stacks\">\n                    <div class=\"stack\" :key=\"stack.id\" :style=\"'transform: rotateY(' + (stack.id * rotateDegree) + 'deg) translateZ(' + rotateDegree * 12 + 'px);'\">\n                        <div class=\"stack-wrapper\">\n                            <img :src=\"stack.url\" alt=\"#\">\n                        </div>\n                    </div>\n                </template>\n            </div>\n        </div>\n        <div class=\"text-center\">\n            <transition name=\"fade\" mode=\"out-in\" >\n                <template v-for=\"stack in stacks\">\n                    <div v-if=\"activeDescription === stack.id\" :key=\"stack.id\">\n                        <p>{{stack.type}}:</p>\n                        <h2>{{stack.title}}</h2>\n                    </div>\n                </template>\n            </transition>\n        </div>\n    </div>\n</template>\n\n<script>\n    import { Stacks } from \"../../mixins/Stacks\";\n\n    export default {\n        name: \"TechSlider\",\n        mixins: [\n            Stacks\n        ],\n        data () {\n            return {\n                curveSlider: 0,\n                rotateDegree: 18,\n                activeDescription: 0\n            }\n        },\n        methods: {\n            rotateSliderLeft () {\n                this.curveSlider = this.curveSlider - this.rotateDegree;\n                if (this.activeDescription !== this.stacks.length -1) {\n                    this.activeDescription++\n                } else {\n                    this.activeDescription = 0\n                }\n            },\n            rotateSliderRight () {\n                this.curveSlider = this.curveSlider + this.rotateDegree;\n                if (this.activeDescription !== 0) {\n                    this.activeDescription--\n                } else {\n                    this.activeDescription = this.stacks.length - 1\n                }\n            }\n        }\n    }\n</script>\n\n<style lang=\"scss\" scoped>\n    .techstack {\n        margin: 60px 0;\n\n        .container {\n            margin: 0 auto;\n            width: 320px;\n            height: 100px;\n            position: relative;\n            perspective: 1000px;\n            overflow: hidden;\n\n            .slider {\n                height: 100%;\n                width: 100%;\n                position: absolute;\n                transform-style: preserve-3d;\n                transition: transform .7s;\n\n                .stack {\n                    display: block;\n                    position: absolute;\n                    top: calc(50% - 25px);\n                    left: calc(50% - 25px);\n\n                    .stack-wrapper {\n                        position: relative;\n\n                        &::before {\n                            content: '';\n                            top: -10px;\n                            left: -18px;\n                            position: absolute;\n                            width: 85px;\n                            height: 70px;\n                            background: rgb(255,255,255);\n                            background: radial-gradient(circle, rgba(255,255,255,1) 0%, rgba(255,255,255,0.6250875350140056) 48%, rgba(255,255,255,0) 100%);\n                            z-index: -1;\n                        }\n\n                        img {\n                            width: 50px;\n                            height: auto;\n                        }\n\n                    }\n                }\n            }\n        }\n    }\n    .fade-enter-active, .fade-leave-active {\n        transition: opacity .5s;\n    }\n    .fade-enter, .fade-leave-to {\n        opacity: 0;\n    }\n\n    p {\n        margin: 5px;\n        font-size: 16px;\n        color: var(--main-bar-color);\n    }\n\n    h2 {\n        margin: 0;\n        font-size: 24px;\n    }\n</style>\n"],"sourceRoot":"src/components/techstack"}]}