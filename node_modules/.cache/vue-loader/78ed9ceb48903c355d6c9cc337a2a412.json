low: visible;\n          }\n\n          &.is-scrolled {\n              height: 0;\n              transition: height .5s ease;\n          }\n\n          .headline {\n              padding: 10px 0 0;\n              height: 100%;\n              width: 100%;\n              overflow: hidden;\n              text-align: center;\n\n              span {\n                  display: inline-block;\n                  font-family: 'Roboto', Helvetica, sans-serif;\n                  font-weight: 900;\n                  font-size: 40px;\n                  color: var(--main-bar-color);\n                  padding: 30px 30px 0;\n                  span {\n                      position: relative;\n                      display: inline-block;\n                      height: 40px;\n                      width: 120px;\n                      padding: 0;\n                      vertical-align: middle;\n                  }\n              }\n\n              svg {\n                  position: absolute;\n                  top: calc(50% - 50px);\n                  left: calc(50% - 60px);\n                  height: 120px;\n                  width: 120px;\n                  animation: spin 25s infinite;\n                  animation-delay: 2s;\n                  fill: var(--main-bar-color);\n\n                  use {\n                      transform: scale(8);\n                  }\n              }\n\n              @keyframes spin {\n                  0% {transform:rotate(0deg);}\n                  75% {transform:rotate(359deg);}\n                  100% {transform:rotate(359deg);}\n              }\n          }\n\n          .menu-toggle {\n              z-index: 4;\n              position: absolute;\n              top: 10px;\n              right: 15px;\n              height: 30px;\n              width: 30px;\n              cursor: pointer;\n\n              svg {\n                  fill: var(--main-bar-color);\n                  width: 30px;\n                  height: 30px;\n                  transition: transform var(--navigation-transition), fill var(--navigation-transition);\n                  transition-delay: .4s;\n              }\n\n              &.is-active {\n\n                  svg {\n                      fill: var(--navigation-mobile-color);\n                      transform: rotate(335deg);\n                      transition: transform var(--navigation-transition), fill var(--navigation-transition);\n                  }\n              }\n          }\n\n          .navigation {\n              z-index: 3;\n              position: absolute;\n              top: var(--main-bar-mobile-height);\n              left: 0;\n              height: 0;\n              width: 100%;\n              display: flex;\n              justify-content: center;\n              align-items: center;\n              transition: height .1s;\n              transition-delay: .3s;\n\n              .overlay-left,\n              .overlay-right {\n                  z-index: 1;\n                  position: fixed;\n                  top: 0;\n                  height: calc(100vh + 10px);\n                  width: calc(80vw + 2px);\n                  transition: transform var(--navigation-transition), opacity var(--navigation-transition);\n                  transition-delay: .4s;\n                  opacity: .1;\n              }\n\n              .overlay-left {\n                  left: -80vw;\n                  background-color: var(--navigation-primary-color);\n              }\n\n              .overlay-right {\n                  right: -80vw;\n                  background-color: var(--navigation-secondary-color);\n              }\n\n              .menu,\n              .contact {\n                  z-index: -2;\n                  position: absolute;\n                  transition: z-index 1.1s;\n              }\n\n              .menu {\n                  top: 10vh;\n              }\n\n              .contact {\n                  position: fixed;\n                  left: 0;\n                  bottom: 10